{
  "private": true,
  "scripts": {
    "start": "umi dev",
    "build": "umi build",
    "postinstall": "umi generate tmp",
    "prettier": "prettier --write '**/*.{js,jsx,tsx,ts,less,md,json}'",
    "test": "umi-test",
    "test:coverage": "umi-test --coverage",
    "commit": "git-cz"
  },

  "lint-staged": {
    "*.{js,jsx,less,md,json}": [
      "prettier --write"
    ],
    "*.ts?(x)": [
      "prettier --parser=typescript --write",
      "eslint --fix",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "dependencies": {
    "@ant-design/pro-layout": "^5.0.12",
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-conventional": "^11.0.0",
    "@umijs/preset-react": "1.x",
    "@umijs/test": "^3.2.16",
    "commitizen": "^4.2.2",
    "git-cz": "^4.7.1",
    "lint-staged": "^10.5.1",
    "prettier": "^2.1.2",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "umi": "^3.2.16",
    "yorkie": "^2.0.0"
  },
  "devDependencies": {
    "@umijs/fabric": "^2.3.1",
    "@umijs/preset-ui": "^2.2.8",
    "eslint": "^7.13.0",
    "husky": "^4.3.0"
  }
}
